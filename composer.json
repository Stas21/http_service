{
    "name": "stas21/http_service",
    "description": "Http service for Laravel",
    "authors": [
        {
            "name": "Stas Khort"
        }
    ],
    "type": "library",
    "require": {
        "illuminate/support": "^5.4",
        "guzzle/http": "^3.9",
        "illuminate/config": "^5.4",
        "mockery/mockery": "^0.9.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "N1X0N\\HttpService\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
